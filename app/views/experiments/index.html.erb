<% provide(:title, 'Experiments') %>

<h1>Experiments</h1>

<table class="mt-5">
   <col width="100">
   <col width="600">
   <col width="150">

  <tr>
    <th>ID</th>
    <th>Parameters</th>
    <th>Updated</th>
  </tr>
    <% @experiments.each do |experiment| %>
        <tr>
            <td><%= experiment.id %></td>
            <td><%= experiment.parameters %></td>
            <td><%= experiment.updated_at.strftime("%m/%d/%Y %H:%M") %></td>
        </tr>
    <% end %>
</table>