<% provide(:title, 'Create a new project') %>
<h1>Create a new project</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3 m-auto">
    <%= form_for(@project) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.submit "Create project", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>